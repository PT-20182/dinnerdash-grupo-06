
<div class="back">
    <%= image_tag("dinner.jpg", :alt => "Dinner", :class => "tamanho") %>
    <div class="log">
			<div class="titulo"><h1 id="title"> Dinnerdash</h1></div>
			<% if notice %>
			<div class="pad">
      <p class="alert alert-success"><%= notice %></p>
    <% end %>
    <% if alert %>
      <p class="alert alert-danger"><%= alert %></p>
	</div>
    <% end %>
			<div class="pad">
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
            <%= devise_error_messages! %>
			</div>
				<div class="pad">
          <p>Email</p>
          <%= f.email_field :email, autofocus: true, autocomplete: "email",  :id=>"email", required: true %>
        </div>
				<% if @minimum_password_length %>
				<% end %>
        <div class="pad">

          <p>Senha</p>
					<%= f.password_field :password, autocomplete: "new-password", :id=>"password", required: true  %>
				</div>

      <div class="pad">
        <p>Confirme sua senha</p>
				<%= f.password_field :password_confirmation, autocomplete: "new-password", :id=>"password_confirmation", required: true %>
				</div>

        <div class="pad">
					<%= f.submit "Cadastrar-se",class:"button" %>
				</div>


        <% end %>
			<div class="linha"></div>
			<div class="pad"><span id="bold">JÃ¡ possui conta?</span></div>
			<div class="pad  ">
			<%=button_to "Sign-in", new_user_session_path,class: "buttonc",:method => "get" %>
		</div>
